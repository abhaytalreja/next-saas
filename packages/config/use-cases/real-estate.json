  "dashboard": {
    "defaultLayout": "crm",
    "widgets": [
      {
        "id": "active-listings",
        "name": "Active Listings",
        "type": "metric",
        "position": { "x": 0, "y": 0, "w": 3, "h": 2 }
      },
      {
        "id": "recent-activities",
        "name": "Recent Activities",
        "type": "activity-feed",
        "position": { "x": 3, "y": 0, "w": 6, "h": 4 }
      },
      {
        "id": "sales-pipeline",
        "name": "Sales Pipeline",
        "type": "chart",
        "position": { "x": 0, "y": 2, "w": 9, "h": 3 }
      }
    ],
    "navigation": [
      { "label": "Dashboard", "href": "/dashboard", "icon": "home" },
      { "label": "Properties", "href": "/properties", "icon": "building" },
      { "label": "Clients", "href": "/clients", "icon": "users" },
      { "label": "Transactions", "href": "/transactions", "icon": "document" },
      { "label": "Reports", "href": "/reports", "icon": "chart" },
      { "label": "Settings", "href": "/settings", "icon": "cog" }
    ],
    "quickActions": [
      { "label": "Add Property", "action": "create-property", "icon": "plus" },
      { "label": "New Client", "action": "create-client", "icon": "user-plus" },
      { "label": "Schedule Showing", "action": "schedule-showing", "icon": "calendar" }
    ]
  },
  "dataModel": {
    "entities": [
      {
        "name": "Property",
        "fields": [
          { "name": "address", "type": "string", "required": true },
          { "name": "price", "type": "number", "required": true },
          { "name": "bedrooms", "type": "number" },
          { "name": "bathrooms", "type": "number" },
          { "name": "squareFootage", "type": "number" },
          { "name": "propertyType", "type": "enum", "options": ["house", "condo", "townhouse", "land"] },
          { "name": "listingStatus", "type": "enum", "options": ["active", "pending", "sold", "withdrawn"] },
          { "name": "description", "type": "string" },
          { "name": "features", "type": "json" },
          { "name": "listingDate", "type": "date" },
          { "name": "mlsNumber", "type": "string" }
        ]
      },
      {
        "name": "Client",
        "fields": [
          { "name": "firstName", "type": "string", "required": true },
          { "name": "lastName", "type": "string", "required": true },
          { "name": "email", "type": "email", "required": true },
          { "name": "phone", "type": "phone" },
          { "name": "clientType", "type": "enum", "options": ["buyer", "seller", "both"] },
          { "name": "budget", "type": "number" },
          { "name": "preapproved", "type": "boolean" },
          { "name": "preferredAreas", "type": "json" },
          { "name": "notes", "type": "string" }
        ]
      },
      {
        "name": "Transaction",
        "fields": [
          { "name": "propertyId", "type": "string", "required": true },
          { "name": "clientId", "type": "string", "required": true },
          { "name": "salePrice", "type": "number", "required": true },
          { "name": "commission", "type": "number" },
          { "name": "status", "type": "enum", "options": ["pending", "under-contract", "closed", "cancelled"] },
          { "name": "closingDate", "type": "date" },
          { "name": "contractDate", "type": "date" }
        ]
      }
    ],
    "relationships": [
      {
        "from": "Transaction",
        "to": "Property",
        "type": "many-to-one",
        "foreignKey": "propertyId"
      },
      {
        "from": "Transaction",
        "to": "Client",
        "type": "many-to-one",
        "foreignKey": "clientId"
      }
    ]
  },
  "features": {
    "enabledFeatures": [
      "property-management",
      "client-crm",
      "transaction-management",
      "mls-integration",
      "document-storage",
      "email-marketing",
      "financial-reporting",
      "calendar-scheduling"
    ],
    "disabledFeatures": [
      "inventory-management",
      "project-management",
      "time-tracking",
      "invoicing"
    ]
  },
  "integrations": {
    "recommended": ["mls", "docusign", "mailchimp", "calendly"],
    "required": ["mls"],
    "optional": ["stripe", "quickbooks", "google-calendar", "zapier"]
  },
  "emailTemplates": {
    "welcome": {
      "subject": "Welcome to RealEstate Pro!",
      "content": "Welcome to RealEstate Pro, {{firstName}}! We're excited to help you manage your real estate business more effectively.",
      "variables": ["firstName", "lastName", "company"]
    },
    "notification": {
      "subject": "New Activity: {{activityType}}",
      "content": "There's been a new {{activityType}} for {{propertyAddress}}. Login to your dashboard to view details.",
      "variables": ["activityType", "propertyAddress", "clientName"]
    },
    "marketing": [
      {
        "subject": "New Property Alert: {{propertyAddress}}",
        "content": "A new property matching your criteria is now available: {{propertyAddress}} for ${{price}}.",
        "variables": ["propertyAddress", "price", "bedrooms", "bathrooms"]
      }
    ]
  },
  "legal": {
    "privacyPolicy": "Standard real estate privacy policy with GDPR compliance",
    "termsOfService": "Real estate specific terms of service",
    "gdprCompliant": true,
    "ccpaCompliant": true,
    "industryCompliance": ["NAR", "MLS", "RESPA"]
  }
}