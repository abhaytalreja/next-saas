    ]
  },
  "dataModel": {
    "entities": [
      {
        "name": "Wallet",
        "fields": [
          { "name": "address", "type": "string", "required": true },
          { "name": "name", "type": "string" },
          { "name": "chain", "type": "enum", "options": ["ethereum", "polygon", "bsc", "avalanche", "fantom", "arbitrum"] },
          { "name": "isActive", "type": "boolean" },
          { "name": "lastSynced", "type": "date" }
        ]
      },
      {
        "name": "Position",
        "fields": [
          { "name": "walletId", "type": "string", "required": true },
          { "name": "protocol", "type": "string", "required": true },
          { "name": "tokenA", "type": "string", "required": true },
          { "name": "tokenB", "type": "string" },
          { "name": "amount", "type": "number", "required": true },
          { "name": "apy", "type": "number" },
          { "name": "value", "type": "number" },
          { "name": "positionType", "type": "enum", "options": ["liquidity", "lending", "borrowing", "staking", "farming"] },
          { "name": "entryDate", "type": "date", "required": true }
        ]
      },
      {
        "name": "Transaction",
        "fields": [
          { "name": "hash", "type": "string", "required": true },
          { "name": "walletId", "type": "string", "required": true },
          { "name": "type", "type": "enum", "options": ["swap", "deposit", "withdraw", "claim", "stake", "unstake"] },
          { "name": "amount", "type": "number", "required": true },
          { "name": "token", "type": "string", "required": true },
          { "name": "gasUsed", "type": "number" },
          { "name": "gasPrice", "type": "number" },
          { "name": "timestamp", "type": "date", "required": true }
        ]
      }
    ],
    "relationships": [
      {
        "from": "Position",
        "to": "Wallet",
        "type": "many-to-one",
        "foreignKey": "walletId"
      },
      {
        "from": "Transaction",
        "to": "Wallet",
        "type": "many-to-one",
        "foreignKey": "walletId"
      }
    ]
  },
  "features": {
    "enabledFeatures": [
      "portfolio-tracking",
      "yield-farming",
      "analytics",
      "risk-management",
      "price-alerts",
      "transaction-history",
      "tax-reporting",
      "strategy-backtesting"
    ],
    "disabledFeatures": [
      "property-management",
      "client-crm",
      "document-storage",
      "invoicing"
    ]
  },
  "integrations": {
    "recommended": ["moralis", "covalent", "defipulse", "coingecko"],
    "required": ["web3provider"],
    "optional": ["1inch", "paraswap", "zapper", "debank"]
  },
  "emailTemplates": {
    "welcome": {
      "subject": "Welcome to DeFi Analytics Pro!",
      "content": "Welcome to DeFi Analytics Pro, {{firstName}}! Connect your wallet to start tracking your DeFi positions.",
      "variables": ["firstName", "lastName"]
    },
    "notification": {
      "subject": "Price Alert: {{tokenSymbol}}",
      "content": "{{tokenSymbol}} has {{direction}} your alert threshold of ${{price}}. Current price: ${{currentPrice}}.",
      "variables": ["tokenSymbol", "direction", "price", "currentPrice"]
    },
    "marketing": [
      {
        "subject": "Weekly DeFi Market Update",
        "content": "Your weekly summary: Portfolio change: {{portfolioChange}}%, Top performer: {{topToken}}.",
        "variables": ["portfolioChange", "topToken", "totalValue"]
      }
    ]
  },
  "legal": {
    "privacyPolicy": "Cryptocurrency and DeFi specific privacy policy",
    "termsOfService": "DeFi analytics terms of service",
    "gdprCompliant": true,
    "ccpaCompliant": true,
    "industryCompliance": ["AML", "KYC", "FATF"]
  }
